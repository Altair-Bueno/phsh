declare -- PHSH_REQUEST_VERB="GET"
declare -- PHSH_REQUEST_PATH="/space/test?x=1"
declare -- PHSH_REQUEST_VERSION="HTTP/1.1"
declare -- PHSH_REQUEST_PATHNAME="/space/test"
declare -- PHSH_REQUEST_HASH=""
declare -- PHSH_REQUEST_SEARCH="?x=1"
declare -- PHSH_REQUEST_BODY=""
declare -A PHSH_REQUEST_HEADERS=([accept]="*/*" [host]="example.com" [user-agent]="TestSuite/1.0" )
declare -A PHSH_REQUEST_SEARCH_PARAMS=([x]="1" )
